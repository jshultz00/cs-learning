// Test array indexing
class Main {
    function void main() {
        var Array arr;
        var int i, sum;

        let arr = Array.new(5);
        let i = 0;

        while (i < 5) {
            let arr[i] = i * 2;
            let i = i + 1;
        }

        let sum = 0;
        let i = 0;

        while (i < 5) {
            let sum = sum + arr[i];
            let i = i + 1;
        }

        do Output.printInt(sum);
        do arr.dispose();

        return;
    }
}
