
function Math.multiply 2
    push constant 0
    pop local 0
    push argument 1
    pop local 1

label MULT_LOOP
    push local 1
    push constant 0
    eq
    if-goto MULT_END

    push local 0
    push argument 0
    add
    pop local 0

    push local 1
    push constant 1
    sub
    pop local 1

    goto MULT_LOOP

label MULT_END
    push local 0
    return
