add               // 10 + 45 = 55
push argument 1   // Push argument[1] (21)
sub               // 55 - 21 = 34
push this 6       // Push this[6] (36)
push this 6       // Push this[6] (36)
add               // 36 + 36 = 72
sub               // 34 - 72 = -38
push temp 6       // Push temp[6] (510)
add               // -38 + 510 = 472
